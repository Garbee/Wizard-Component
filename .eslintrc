{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-underscore-dangle": [
            0
        ],
        "globals": [
            "componentHandler": true
        ]
    },
    "env": {
        "browser": true
    }
}
